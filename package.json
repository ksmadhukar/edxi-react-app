{
  "name": "setup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p watch:css start:react",
    "build": "run-p build:clean build:css build:react",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "build:clean": "rimraf ./build",
    "build:css": "postcss src/assets/styles/app.css -o public/app.css",
    "watch:css": "postcss src/assets/styles/app.css -o public/app.css -w",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "autoprefixer": "^9.7.1",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "tailwindcss": "^1.1.3"
  }
}
